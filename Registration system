#include <iostream>
#include<bits/stdc++.h>

using namespace std;

int main() {
	// your code goes here
int n;
cin>>n;
map<string, int> m;
int t=n;
string c;
int l;
for(int p=0;p<n;p++) 
{
cin>>c;

if(m[c]==0) 
{cout<<"OK"; 
m[c]++;}

else
{cout<<c<<m[c];
m[c]++;}

cout<<endl;

}






	return 0;
}
